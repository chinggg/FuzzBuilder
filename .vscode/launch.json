  "version": "0.2.0",
  "configurations": [
    {
      "name": "expat seed",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceFolder}/build/fuzzbuilder",
      "args": ["seed", "${workspaceFolder}/exp/expat/seed.conf"],
      "cwd": "${workspaceFolder}/exp/expat/source/libexpat"
    },
    {
      "name": "expat exec",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceFolder}/build/fuzzbuilder",
      "args": ["exec", "${workspaceFolder}/exp/expat/XML_Parse.conf"],
      "cwd": "${workspaceFolder}/exp/expat/source/libexpat/expat/tests"
    }
  ]
}